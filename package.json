    "jsdom": "^27.0.0",
    "netlify-cli": "^23.8.0",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
